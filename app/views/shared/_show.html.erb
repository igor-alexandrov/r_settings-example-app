<div class="b-setting">
  <div class="b-setting_attribute">
    <span class="b-setting_attribute__title">Key:</span>
    <span class="b-setting_attribute__value"><%= setting.key %></span>
  </div>
  <div class="b-setting_attribute">
    <span class="b-setting_attribute__title">Data type:</span>
    <span class="b-setting_attribute__value"><%= setting.data_type %></span>
  </div>
  <div class="b-setting_attribute">
    <span class="b-setting_attribute__title">DB Value:</span>
    <span class="b-setting_attribute__value"><%= setting.value %></span>
  </div>
  <div class="b-setting_attribute">
    <span class="b-setting_attribute__title">Cached Value:</span>
    <span class="b-setting_attribute__value"><%= cached_value %></span>
  </div>
  <div class="b-setting_attribute">
    <span class="b-setting_attribute__title">Description:</span>
    <span class="b-setting_attribute__value"><%= setting.description %></span>
  </div>
</div>